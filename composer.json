{
    "name": "codicastudio/directory-cleanup",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/directory-cleanup",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0",
        "nesbot/carbon": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench":"^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\DirectoryCleanup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\DirectoryCleanup\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\DirectoryCleanup\\DirectoryCleanupServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
